{
  "version": 2,
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.py" },
    { "src": "/(.*)\\.(js|css|png|ico|svg)", "dest": "/public/$1.$2" },
    { "src": "/favicon.(.*)", "dest": "/public/scorpios.png" },
    { "src": "/", "dest": "/public/index.html" },
    { "src": "/(.*)", "dest": "/public/index.html" }
  ]
}

