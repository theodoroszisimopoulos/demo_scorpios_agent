{
  "version": 2,
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.py" },
    { "src": "/(.*)\\.(js|css|png|ico|svg|json|txt|webmanifest)", "dest": "/$1.$2" },
    { "src": "/_next/(.*)", "dest": "/_next/$1" },
    { "src": "/favicon.(.*)", "dest": "/scorpios.png" },
    { "src": "/", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}

